pipeline {
    agent any

    environment {
            CI = 'true'
        }
    stages {
        stage('Pre Build') {
            steps {
                sh './jenkins/scripts/before_install.sh'
               
            }
        }
        stage('Build') {
            steps {
                sh 'npm run-script build'
            }
        }
        stage('Post Build') {
                    steps {
                      sh './jenkins/scripts/after_install.sh'
                    }
                }
        stage('Launch') {
                    steps {
                       
                        sh './jenkins/scripts/start_server.sh'
                    }
                }

    }
}