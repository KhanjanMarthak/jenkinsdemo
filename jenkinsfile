pipeline {
    agent{
        none
    }
    stages {
        stage('Pre Build') {
            steps {
                    echo 'this is prebuild stage'
               
            }
        }
        stage('Build') {
            steps {
                echo 'this is BuildStage'    
            }
        }
        stage('Post Build') {
                    steps {
                     echo 'this is post build stage'    
                    }
                }
        stage('Launch') {
                    steps {
                       echo 'this is Launch stage'    
                    }
                }

    }
}